<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="user">
    <unique-constraints>
      <unique-constraint name="email" columns="email"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstname" type="string" column="firstname" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pwd" type="string" column="pwd" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="salt" type="string" column="salt" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="token" type="string" column="token" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
